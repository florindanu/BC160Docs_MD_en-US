{
  "build": {
    "content":
      [
        {
          "files": [
            "**/*.md",
            "**/*.yml"
          ],
          "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
          ]
        }
      ],
    "resource": [
        {
          "files": ["**/media/**"],
          "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
        }
    ],
    "globalMetadata": {
        "ROBOTS": "NOINDEX, NOFOLLOW"
    },
    "externalReference": [
    ],
    "template": "../Templates/HelpServer",
    "dest": "c:/output",
    "markdownEngineName": "markdig"
  }
}